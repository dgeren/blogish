    <%- include('partials/header'); -%>

      <div id="container-gutter" class="medium">
        <%- include('partials/gutter'); -%>
      </div> <!-- close #container-gutter -->

      <div id="container-content">
        <h3 id="page-title"><%= publish ? "" : "Unpublished " %>Posts</h3>

        <main>
          <% entries.forEach(entry => { %>
            <% const { title, description, slug, preview, tagHTML = "", dateDisplay, author } = entry; %>
          <div class="card">
            <h4 class="preview-el">
            <a href="/reader/slug/<%= slug %>"><%- title %></a>
            <% if(user) { %>
            <a href="/editor/slug/<%= slug %>" class="preview-el medium button">&nbsp; [ &#9998;&nbsp;edit ]</a>
            <% } %>
            </h4>
            <h5 class="preview-el"><%- description %></h5>
            <h5 class="preview-el"><%= publish ? dateDisplay : "Unpublished" %></h5>
            <p class="preview-el"><%- preview %></p>
            <p class="preview-el small-text"><%- tagHTML %></p>
          </div>
          <% }); %>
        </main>
        <div class="pagination">
          <%- include('partials/navigateEntries'); %>
        </div>
      </div> <!-- close #container-content -->

      <div id="container-footer">
        <%- include('partials/footer') -%>
      </div> <!-- close #container-footer -->
    </div>
      
    <%- include('partials/scripts') %>
</body>
</html>